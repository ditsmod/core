{
  "name": "@ditsmod/testing",
  "type": "module",
  "version": "2.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "This is a module for Ditsmod applications",
  "module": "dist/index.js",
  "imports": {
    "#src/*": "./dist/*"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index",
  "types": "dist/index",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json && tsc -b tsconfig.test.json",
    "clean": "rm -rf dist*",
    "test": "tsc -b tsconfig.test.json && yarn esm-jest",
    "esm-jest": "node --experimental-vm-modules --no-warnings=ExperimentalWarning ../../node_modules/jest/bin/jest.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@ditsmod/core": "^2.48.0",
    "@ditsmod/router": "^2.7.0"
  },
  "gitHead": "c64c99dc2b6f65bb3e7027e8f8dceeca7a8c2172"
}
