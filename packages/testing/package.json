{
  "name": "@ditsmod/testing",
  "type": "module",
  "version": "3.0.0-alpha.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "This is a module for Ditsmod applications",
  "module": "dist/index.js",
  "imports": {
    "#app/*": "./dist/*"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "tsc -b tsconfig.unit.json && yarn vitest",
    "rebuild": "cd ../.. && yarn clean && cd - && yarn build"
  },
  "keywords": [
    "ditsmod"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@ditsmod/core": "^3.0.0-alpha.1"
  },
  "devDependencies": {
    "npm-run-all": "*",
    "typescript": "*",
    "vitest": "*"
  }
}
