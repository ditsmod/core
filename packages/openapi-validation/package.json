{
  "name": "@ditsmod/openapi-validation",
  "type": "module",
  "version": "2.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "Ditsmod module to support simple OpenAPI validation",
  "module": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index",
  "types": "dist/index",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "yarn build && yarn esm-jest",
    "esm-jest": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js"
  },
  "keywords": [],
  "author": "Костя Третяк",
  "license": "MIT",
  "dependencies": {
    "@ts-stack/cookies": "^1.0.3",
    "ajv": "^8.11.0"
  },
  "peerDependencies": {
    "@ditsmod/core": "^2.47.0",
    "@ditsmod/i18n": "^2.8.0",
    "@ditsmod/openapi": "^2.20.0"
  },
  "gitHead": "c64c99dc2b6f65bb3e7027e8f8dceeca7a8c2172"
}
