{
  "name": "@ditsmod/i18n",
  "type": "module",
  "version": "2.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "Ditsmod module to integration with i18n (internalization)",
  "module": "dist/index.js",
  "scripts": {
    "test": "tsc -b tsconfig.test.json && yarn esm-jest",
    "esm-jest": "node --experimental-vm-modules --no-warnings=ExperimentalWarning ../../node_modules/jest/bin/jest.js",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*"
  },
  "imports": {
    "#types/*": "./dist/types/*",
    "#src/*": "./dist/*"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index",
  "types": "dist/index",
  "directories": {
    "lib": "src"
  },
  "keywords": [],
  "author": "Костя Третяк",
  "license": "MIT",
  "peerDependencies": {
    "@ditsmod/core": "^2.48.0"
  },
  "gitHead": "c64c99dc2b6f65bb3e7027e8f8dceeca7a8c2172"
}
