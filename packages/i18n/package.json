{
  "name": "@ditsmod/i18n",
  "type": "module",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "Ditsmod module to integration with i18n (internalization)",
  "module": "dist/index.js",
  "scripts": {
    "test": "tsc -b tsconfig.unit.json && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*"
  },
  "imports": {
    "#types/*": "./dist/types/*",
    "#src/*": "./dist/*"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "ditsmod"
  ],
  "author": "Костя Третяк",
  "license": "MIT",
  "peerDependencies": {
    "@ditsmod/core": "^3.0.0",
    "@ditsmod/routing": "^3.0.0"
  },
  "gitHead": "c64c99dc2b6f65bb3e7027e8f8dceeca7a8c2172"
}
