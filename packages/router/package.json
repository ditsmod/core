{
  "name": "@ditsmod/router",
  "type": "module",
  "version": "2.5.0",
  "author": "Костя Третяк <ktretiak.in.ua@gmail.com>",
  "homepage": "https://ditsmod.github.io/en/",
  "description": "Router module for Ditsmod applications",
  "module": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "npm run build && node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "bench": "npm run build && node --expose-gc dist/bench.js"
  },
  "keywords": [
    "router",
    "fast",
    "radix",
    "tree",
    "typescript"
  ],
  "devDependencies": {
    "find-my-way": "^7.6.2",
    "koa-tree-router": "^0.12.1",
    "trek-router": "^1.2.0"
  },
  "peerDependencies": {
    "@ditsmod/core": "^2.33.0"
  },
