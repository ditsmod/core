{
  "name": "@ditsmod/openapi",
  "type": "module",
  "version": "2.20.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "This is module for Ditsmod framework to support OpenAPI",
  "module": "dist/index.js",
  "imports": {
    "#types/*": "./dist/types/*",
    "#utils/*": "./dist/utils/*",
    "#decorators/*": "./dist/decorators/*",
    "#constants": "./dist/constants.js"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index",
  "types": "dist/index",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "npm run build && npm run esm-jest",
    "esm-jest": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "gitHead": "c64c99dc2b6f65bb3e7027e8f8dceeca7a8c2172",
  "dependencies": {
    "@ts-stack/openapi-spec": "^3.1.5",
    "swagger-ui-dist": "^5.4.2",
    "yaml": "^2.3.1"
  },
  "peerDependencies": {
    "@ditsmod/core": "^2.47.0"
  },
  "devDependencies": {
    "@types/swagger-ui-dist": "^3.30.1"
  }
}