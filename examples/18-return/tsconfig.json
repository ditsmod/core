{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist/tmp",
    "baseUrl": "./",
    "tsBuildInfoFile": "dist/ts.tsbuildinfo",
    "paths": {
      "@src/*": ["./src/*"],
      "@ditsmod/core": ["../../packages/core/src"],
      "@ditsmod/cors": ["../../packages/cors/src"],
      "@ditsmod/return": ["../../packages/return/src"],
      "@ditsmod/router": ["../../packages/router/src"],
      "@ditsmod/openapi": ["../../packages/openapi/src"],
      "@ditsmod/jwt": ["../../packages/jwt/src"],
      "@ditsmod/i18n": ["../../packages/i18n/src"],
      "@dict/first/*": ["./src/app/first/locales/current/_base-en/*"],
      "@ditsmod/testing": ["../../packages/testing/src"],
      "@dict/second/*": ["./src/app/second/locales/current/_base-en/*"]
    }
  },
  "include": ["src", "test"],
  "references": [
    { "path": "../../packages/core/tsconfig.build.json" },
    { "path": "../../packages/router/tsconfig.build.json" },
    { "path": "../../packages/return/tsconfig.build.json" },
    { "path": "../../packages/testing/tsconfig.build.json" },
  ]
}
