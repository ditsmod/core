{
  "name": "example16",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node --watch --enable-source-maps dist/main.js",
    "start:dev": "concurrently -r 'npm:watch' 'npm:start'",
    "watch": "npm run build -- --watch",
    "clean": "rimraf dist*",
    "test": "tsc -b tsconfig.e2e.json && yarn vitest",
    "build": "tsc -b tsconfig.build.json"
  },
  "imports": {
    "#app/*": "./dist/app/*"
  },
  "devDependencies": {
    "typescript": "*",
    "vitest": "*"
  }
}
